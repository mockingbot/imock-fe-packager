{
  "name": "imock-fe-packager",
  "version": "1.1.0",
  "author": "mockingbot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mockingbot/imock-fe-packager.git"
  },
  "bin": "bin/index.js",
  "main": "library/index.js",
  "scripts": {
    "library": "cross-env NODE_ENV=production webpack --config config/library.conf.js",
    "library-dev-watch": "cross-env NODE_ENV=development webpack --watch --config config/library.conf.js",
    "build": "npm run library",
    "start": "npm run library-dev-watch",
    "start-dev": "cross-env NODE_ENV=development node library/index.js",
    "start-dev-inspect": "cross-env NODE_ENV=development node --inspect-brk library/index.js",
    "prepare": "npm run build"
  },
  "engines": {
    "node": "^8.0.0",
    "npm": "^5.3.0"
  },
  "devDependencies": {
    "aws-sdk": "^2.94.0",
    "babel-core": "^6.25.0",
    "babel-eslint": "^7.2.2",
    "babel-loader": "^7.1.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^5.0.4",
    "eslint": "^4.4.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "webpack": "^3.4.1"
  }
}
